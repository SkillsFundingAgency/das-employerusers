      <target type="File" name="Disk" fileName="${basedir}/App_Data/logs/${shortdate}.log" layout="${longdate} [${uppercase:${level}}] [${logger}] - ${message} \n ${exception}" />
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="Disk" />
    </rules>
  </nlog>
